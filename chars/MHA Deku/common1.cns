stateno=970
pos=0,10
postype=p1
size.xscale=1
size.yscale=1
supermovetime=999
pausemovetime=999

[State -3, Full Cowl 20% ON]
type = ChangeAnim
trigger1 = var(50) = 1
trigger1 = anim != 0
value = 0

[State -3, Full Cowl 20% OFF]
type = ChangeAnim
trigger1 = NumHelper(2015) = 1 && var(50) = 0 && Time = 0
